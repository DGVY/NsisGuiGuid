    <UnICOPath>
        <def>!define UN_ICON_PATH </def>
        <value>"${NSISDIR}\Contrib\Graphics\Icons\modern-uninstall.ico"</value>
    </UnICOPath>
    
    <!--安装界面头部图片路径-->
    <InHeadBmpPath>
        <def>!define IN_HEAD_BMP_PATH </def>
        <value>"${NSISDIR}\Contrib\Graphics\Header\nsis3-branding.bmp"</value>
    </InHeadBmpPath>
    <UnHeadBmpPath>
        <def>!define UN_HEAD_BMP_PATH </def>
