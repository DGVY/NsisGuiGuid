        <def>!define COMPANY_NAME </def>
        <value>"HZLH"</value>
    </Company>
    
    <!--版本-->
    <Version>
        <def>!define VERSION </def>
        <value>"v1.0.0"</value>
    </Version>
    
    <!--项目-->
    <Project>
        <def>!define PROJECT_NAME </def>
        <value>""</value>
    </Project>
    
    <!--语言-->
    <language>
        <def>!define SET_LANGUAGE </def>
        <value>"SimpChinese"</value>
